{
  "compilerOptions": {
    "target": "esnext",
    "useDefineForClassFields": true,
    "module": "esnext",
    "moduleResolution": "node",
    "strict": true,
    "jsx": "preserve",
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "lib": ["esnext", "dom"],
    "noImplicitAny":false,
    "baseUrl":"./",
    "paths": {
      "@/*": ["src/*"]
     }
  },
  "include": ["src/**/*.ts","src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue", "src/config/pinia.js", "src/components/Searchs/search.jsx"],
  "references": [{ "path": "./tsconfig.node.json" }]
}
